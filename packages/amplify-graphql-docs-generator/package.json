{
  "name": "amplify-graphql-docs-generator",
  "version": "1.0.8",
  "main": "lib/index.js",
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "bin": {
    "amplify-graphql-docs-generator": "./bin/cli"
  },
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci -i",
    "test-watch": "jest --watch",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf ./lib",
    "lint": "tslint -p ./tslint.json",
    "format": "prettier --write ./src/**/*.ts ./__tests__/**/*.ts"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "@types/handlebars": "4.1.0",
    "@types/jest": "^23.3.1",
    "@types/node": "8.0.28",
    "@types/prettier": "^1.13.2",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "camel-case": "^3.0.0",
    "graphql": "^14.2.1",
    "handlebars": "^4.1.1",
    "pascal-case": "^2.0.1",
    "prettier": "1.16.4",
    "yargs": "^13.2.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node"
    ]
  }
}
